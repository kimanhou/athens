-transavia/check-in/login/' checked={false} />\n                <CheckListItem title='Accommodation' text='Airbnb booked' checked={true} />\n                <CheckListItem title='Google maps' text='Download offline map of Athens' checked={false} />\n                <CheckListItem title='Currency' text='Euro' checked={true} />\n                <CheckListItem title='Visa' text='No need for EU' checked={true} />\n                <CheckListItem title='Electricity' text='Same as France' checked={true} />\n            </div>\n        </ContentSection>\n    );\n}\n\nexport default CheckList;","import React from 'react';\nimport './FromAirportIllustrationDot.scss';\n\ninterface IFromAirportIllustrationDotProps {\n    title: string;\n    small ?: boolean;\n}\n\nconst FromAirportIllustrationDot : React.FC<IFromAirportIllustrationDotProps> = props => {\n    const smallClassname = props.small ? 'small' : '';\n    return (\n        <div className={`from-aiport-illustration-dot ${smallClassname}`}>\n            <div className={`title`}>\n                {props.title}\n            </div>\n            <div className={`dot`}></div>\n        </div>\n    );\n}\n\nexport default FromAirportIllustrationDot;","export default __webpack_public_path__ + \"static/media/metro.ae78c49a.png\";","export default __webpack_public_path__ + \"static/media/bus.7180ba48.png\";","import React from 'react';\nimport './FromAirportIllustration.scss';\nimport FromAirportIllustrationDot from './FromAirportIllustrationDot';\nimport metro from '../../../../images/metro.png';\nimport bus from '../../../../images/bus.png';\n\nconst FromAirportIllustration : React.FC = props => {\n    return (\n        <div className={`from-airport-illustration`}>\n            <FromAirportIllustrationDot title='AIRPORT' />\n            <div className={`from-airport-illustration-part one`}>\n                <div className={`from-airport-illustration-part-option metro-3`}>\n                    <img className={`metro transport`} sr